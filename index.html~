  <body>
    <script>
      function computerPlay() {
      let getRandom = Math.floor(Math.random() * 3);
        if (getRandom === 0) {
          return "Rock";
        }
        if (getRandom === 1) {
          return "Paper";
        }
        else {
            return "Scissors";
        }
      }
    </script>
  </body>
